    <TextView
        android:id="@+id/textViewHeight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/height"
        android:textColor="@color/dark_text"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintVertical_bias="0.4"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintWidth_percent="0.35"/>

    <TextView
        android:id="@+id/textViewWeight"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/weight"
        android:textColor="@color/dark_text"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintVertical_bias="0.4"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintWidth_percent="0.35"/>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayoutHeight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:hintEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.455"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.35"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/editTextHeight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:focusable="true"
            android:textColor="@color/dark_text"
            android:hint="@string/your_height"/>
        <!--            android:hint="@string/height"-->
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayoutWeight"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:hintEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.455"
        app:layout_constraintHorizontal_bias="0.86"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.35"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <EditText
            android:id="@+id/editTextWeight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="10"
            android:focusable="true"
            android:textColor="@color/dark_text"
            android:hint="@string/your_weight"/>
        <!--            android:hint="@string/weight"-->
    </com.google.android.material.textfield.TextInputLayout>

    <TableLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintVertical_bias="0.54"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintWidth_percent="0.82">

        <TableRow>

            <TextView
                android:id="@+id/textViewLevelOfPhysicalActivity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/level_of_physical_activity"
                android:textColor="@color/dark_text"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintHeight_percent="0.145"
                app:layout_constraintVertical_bias="0.54"
                app:layout_constraintHorizontal_bias="0.14"
                app:layout_constraintWidth_percent="0.41"/>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:layout_weight="1">
                <ImageButton
                    android:id="@+id/imageButtonQuestionAfter"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:src="@drawable/question_circle"
                    android:background="?attr/selectableItemBackground"
                    android:contentDescription="@string/logo_description"
                    app:layout_constraintStart_toEndOf="@+id/textViewLevelOfPhysicalActivity"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.54"
                    app:layout_constraintHorizontal_bias="0.02"
                    android:layout_marginStart="3dp"/>
            </LinearLayout>
        </TableRow>
    </TableLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayoutLevelOfPhysicalActivity"
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:hintEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.605"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextViewLevelOfPhysicalActivityAfter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:inputType="none"
            android:focusable="true"
            android:clickable="true"
            android:textColor="@color/dark_text"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:hint="@string/choose_level_of_physical_activity"/>
        <!--            android:hint="@string/level_of_physical_activity"-->
    </com.google.android.material.textfield.TextInputLayout>


    <TextView
        android:id="@+id/textViewGoal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/goal"
        android:textColor="@color/dark_text"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHeight_percent="0.145"
        app:layout_constraintVertical_bias="0.68"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintWidth_percent="0.82"/>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayoutGoal"
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:hintEnabled="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.75"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.82"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextViewGoalAfter"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:inputType="none"
            android:focusable="true"
            android:clickable="true"
            android:textColor="@color/dark_text"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:maxLines="1"
            android:ellipsize="end"
            android:singleLine="true"
            android:hint="@string/choose_goal"/>
        <!--            android:hint="@string/goal"-->
    </com.google.android.material.textfield.TextInputLayout>


    <Button
        android:id="@+id/buttonSave"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/save"
        android:textSize="20sp"
        android:onClick="toMain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.85"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHeight_percent="0.08"
        app:layout_constraintWidth_percent="0.82"/>




</androidx.constraintlayout.widget.ConstraintLayout>


